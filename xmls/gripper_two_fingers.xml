            <inertial pos="0 0 0" mass="0.001" diaginertia="1.66667e-08 1.66667e-08 1.66667e-08" />
            <camera name="gripper_camera" pos="0.1 0 0.05" euler="0 -1.5707963 -1.5707963" fovy="135" />
